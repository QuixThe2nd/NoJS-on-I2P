{
	"manifest_version": 2,
	"name": "NoJS on I2P",
	"version": "1.0.1",
	"description": "Disables JavaScript on .i2p domains",
	"permissions": ["webRequest", "webRequestBlocking", "*://*.i2p/*", "storage"],
	"background": {
		"scripts": ["background.js"]
	},
	"options_ui": {
		"page": "options.html"
	}
}
